{
  "name": "appseed",
  "version": "1.2.0",
  "description": "Starter Kit for ES6 workflow",
  "main": "server/index.js",
  "scripts": {
    "prestart": "npm i",
    "start": "cd /var/nodejs && NODE_ENV=production node_modules/.bin/nodemon server"
  },
 "author": "Phil Willis <pdotwdot@gmail.com> (http://www.philwillis.pw/)",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.18.2",
    "chalk": "^2.1.0",
    "compression": "^1.7.1",
    "debug": "^3.1.0",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "minimatch": "^3.0.4",
    "nodemon": "^1.12.1",
    "nsp": "^2.8.1"
  },
   "nyc": {
    "include": ["www/js/**/*.js"],
    "instrument": false,
    "sourceMap": false
  },
  "jest": {
    "roots": ["www/react"],
    "testResultsProcessor": "./node_modules/jest-html-reporter"
  },
  "jest-html-reporter": {
    "pageTitle": "React Test Report",
    "outputPath": "tests-reports/react-coverage.html",
    "styleOverridePath": "tools/react-coverage.css"
  }
}